{
  "name": "mern-auth",
  "version": "1.0.0",
  "type": "module",
  "description": "MERN authentication system",
  "main": "server.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node backend/index.js",
    "server": "node backend/index.js",
    "client": "npm run start --prefix frontend",
    "dev": "nodemon backend/index.js",
    "app": "concurrently \"npm run index\" \"npm run client\"",
    "build": "npm run build --prefix frontend",
    "winBuild": "npm run winBuild --prefix frontend"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
